{
  "name": "@intouchg/components",
  "version": "0.1.1",
  "description": "React component library for the Intouch Design System",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/intouchg/components",
  "author": "Intouch Group",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist && npm run typegen",
    "build": "npm run lint && rollup -c",
    "watch": "rollup -c -w",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false --strict --jsx react",
    "typegen": "tsc",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@babel/runtime": "7.13.10",
    "@intouchg/theme": "latest",
    "@styled-system/should-forward-prop": "5.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/eslint-parser": "7.14.7",
    "@babel/eslint-plugin": "7.14.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@intouchg/eslint-config": "latest",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@types/styled-components": "5.1.14",
    "@types/styled-system": "5.1.13",
    "@types/styled-system__should-forward-prop": "5.1.2",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "babel-plugin-styled-components": "1.12.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "react": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.41.1",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-terser": "7.0.2",
    "styled-components": "5.3.0",
    "styled-system": "5.1.5",
    "typescript": "4.3.5"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-spring": ">=9.2.5",
    "styled-components": ">=5.3.0",
    "styled-system": ">=5.1.5",
    "typescript": ">=4.3.5"
